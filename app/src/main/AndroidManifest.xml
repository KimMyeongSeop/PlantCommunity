<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.androidtown.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@drawable/unnamed"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.androidtown.myapplication.LoginActivity">
        </activity>
        <activity
            android:name="org.androidtown.myapplication.SignupActivity">
        </activity>
        <activity
            android:name="org.androidtown.myapplication.SignupMyPlant">
        </activity>
        <activity
            android:name="org.androidtown.myapplication.Signupinfo">
        </activity>
        <activity
            android:name="org.androidtown.myapplication.Register_pic"
            android:theme="@style/Theme.AppCompat.DayNight.Dialog">
        </activity>
        <activity
            android:name="org.androidtown.myapplication.Resister_rpi">
        </activity>
        <activity
            android:name="org.androidtown.myapplication.Myplant">
        </activity>
        <activity
            android:name="org.androidtown.myapplication.Diary">
        </activity>
        <activity
            android:name="org.androidtown.myapplication.Click_a_date"
            android:theme="@style/Theme.AppCompat.DayNight.Dialog">
        </activity>
    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
</manifest>